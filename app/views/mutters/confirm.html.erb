<h3>以下の内容で、送信する。</h3>

<%= form_for(@mutter) do |f| %>
  <%= f.hidden_field :content %>
  <p>本文:<%= @mutter.content %></p>
  <%= f.submit "登録する" %>
<% end %>

<%= form_for(@mutter, url: new_mutter_path, method: 'get') do |f| %>
  <%= f.hidden_field :content %>
  <%= f.submit "もどる", name: 'back' %>
<% end %>